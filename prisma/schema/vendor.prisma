model vendor {
  id        String   @id @default(cuid())
  firstname String?
  lastname  String?
  company   String?
  email     String?
  phone     String?
  address   String?
  city      String?
  state     String?
  zip       String?
  country   String?
  website   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
